@model List<DemoApi.Models.Markalar>

<h2>Gruplar</h2>

<table class="table">
    <thead>
        <tr>
            <th>MarkaId</th>
            <th>Marka</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var marka in Model)
        {
            <tr>
                <td>@marka.MarkaId</td>
                <td>@marka.Marka</td>
            </tr>
        }
    </tbody>
</table>
